{
  "name": "@frameless/utrecht-icon-library",
  "version": "0.0.0-development",
  "description": "",
  "repository": {
    "type": "git+ssh",
    "url": "https:git@github.com:frameless/opengemeenten-iconset.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@frameless"
  },
  "workspaces": [
    "libs/*"
  ],
  "scripts": {
    "build": "npm-run-all build:**",
    "build:iwcs": "npm run build -w  @frameless/icon-web-component-stencil",
    "build:iwcr": "npm run build -w  @frameless/icon-web-component-react",
    "build:iwcv": "npm run build -w  @frameless/icon-web-component-vue",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm-run-all --continue-on-error lint:**",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:check": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx,.vue' --report-unused-disable-directives .",
    "lint:package-lock": "npm ls",
    "lint:prettier": "prettier --check .",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:** prettier",
    "lint-fix:css": "stylelint --fix '**/*.{css,scss}'",
    "lint-fix:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx,.vue' --fix --report-unused-disable-directives .",
    "prettier": "prettier --write .",
    "semantic-release": "semantic-release -e semantic-release-monorepo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/node": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.30.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "semantic-release-monorepo": "^7.0.5",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.0.8"
  }
}
