{
  "name": "@frameless/icon-web-component-stencil",
  "version": "0.0.0-development",
  "private": false,
  "description": "Icon Web Component Stencil",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/icons/icons.esm.js",
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:frameless/opengemeenten-iconset.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "prebuild": "rimraf dist loader www",
    "build": "stencil build --docs",
    "clean": "rimraf node_modules",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "svgo": "svgo -f ./src/svg -o ./src/optimized-svgs",
    "postgc": "npm run build",
    "pregc": "rimraf src/components/icons",
    "gc": "node .generate_component $1 --prefix utrecht-icon --path src/components/icons",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:** prettier",
    "lint-fix:css": "stylelint --fix '**/*.{css,scss}'",
    "lint-fix:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx,.vue' --fix",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@stencil/core": "^2.16.0"
  },
  "devDependencies": {
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/vue-output-target": "^0.6.0",
    "@types/jest": "^27.0.3",
    "@types/uuid": "^8.3.4",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "minimist": "^1.2.6",
    "puppeteer": "^14.2.1",
    "stencil-inline-svg": "^1.1.0",
    "svgo": "^2.8.0",
    "uuid": "^8.3.2"
  },
  "license": "MIT"
}
